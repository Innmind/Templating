{
    "name": "innmind/templating",
    "type": "library",
    "description": "Templating",
    "keywords": ["templating"],
    "homepage": "http://github.com/Innmind/Templating",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Templating/issues"
    },
    "require": {
        "php": "~7.2",
        "innmind/immutable": "^2.8",
        "innmind/stream": "^1.4",
        "innmind/filesystem": "^3.2",
        "twig/twig": "^2.5",
        "innmind/url": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Templating\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Templating\\": "tests/",
            "Fixture\\Innmind\\Templating\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "giorgiosironi/eris": "^0.11.0",
        "innmind/compose": "^1.7"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    }
}
