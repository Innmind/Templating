{
    "name": "innmind/templating",
    "type": "library",
    "description": "Templating",
    "keywords": ["templating"],
    "homepage": "http://github.com/Innmind/Templating",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Templating/issues"
    },
    "require": {
        "php": "~8.1",
        "innmind/immutable": "~4.0",
        "innmind/filesystem": "~5.0",
        "twig/twig": "~3.0",
        "innmind/url": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Templating\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Templating\\": "tests/",
            "Fixture\\Innmind\\Templating\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.4",
        "innmind/coding-standard": "~2.0"
    }
}
